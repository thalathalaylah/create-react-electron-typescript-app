{
  "name": "create-react-electron-typescript-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.7",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "package": "yarn build && electron-packager . my-app --platform=darwin --arch=x64 --prune --out=release --overwrite",
    "package-all": "yarn build && electron-packager . my-app --platform=all --arch=x64 --prune --out=release --overwrite",
    "dev": "nf start -j dev_env/Procfile -e dev_env/.dotenv"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "^3.4.4",
    "electron": "^11.5.0",
    "electron-packager": "^13.1.1",
    "foreman": "^3.0.1",
    "uuid": "^3.3.2"
  },
  "main": "src_main/entrypoint.js",
  "homepage": "./"
}
